<div class="col-lg-12 grid-margin stretch-card">
<div class="card">
  <div class="card-body">
    <h4 class="card-title">Statistics by apply</h4>
    <p class="card-description">
      Add class <code>.table-dark</code>
    </p>
    <div class="table-responsive">
      <table class="table table-dark">
        <thead>
          <tr>
            <th>
              #
            </th>
            <th>
              Job
            </th>
            <th>
              Quantity Apply
            </th>
          
          </tr>
        </thead>
        <tbody>
        <% @sta.each do |s| %>
          <tr>
            <td>
              <%=s.id%>
            </td>
            <td>
              <%=s.name%>
            </td>
            <td>
            <%=s.qty_apply%>
            </td>
            
          </tr>
        <% end %>
 
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
</div>